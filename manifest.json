{
  "manifest_version": 3,
  "name": "Multi-Blinks",
  "version": "1.0.0",
  "action": { "default_popup": "index.html" },
  "host_permissions": [
    "https://twitter.com/*",
    "https://x.com/*",
    "https://www.facebook.com/*",
    "https://www.twitch.tv/*",
    "https://www.reddit.com/*",
    "https://www.youtube.com/*",
    "https://github.com/*"
  ]
}
